{
  "name": "react-webpack-boilerplate",
  "version": "1.0.0",
  "description": "React webpack boilerplate.",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && node_modules/.bin/webpack --config webpack.config.production.js",
    "clean": "rm -rf dist coverage",
    "coveralls": "npm run test && cat ./coverage/lcov/lcov.info | coveralls",
    "lint": "node_modules/.bin/eslint ./src",
    "start": "node server.js",
    "test": "npm run lint && node_modules/.bin/karma start --single-run --no-auto-watch karma.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeantimex/react-webpack-boilerplate.git"
  },
  "keywords": [
    "react",
    "boilerplate",
    "mocha",
    "chai",
    "sinon",
    "isparta",
    "scss",
    "es6",
    "babel",
    "webpack",
    "karma"
  ],
  "author": "Yong Su (https://github.com/jeantimex)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeantimex/react-webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/jeantimex/react-webpack-boilerplate",
  "dependencies": {
    "babel-runtime": "^6.9.2",
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel": "^6.5.2",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.11.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.11",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-node": "^0.2.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "isparta-loader": "^2.0.0",
    "istanbul": "^0.4.4",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon-chai": "^1.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-threshold-reporter": "^0.1.15",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.5.3",
    "mocha-loader": "^0.7.1",
    "node-sass": "^3.8.0",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^4.0.0",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
